# USE_CLIENTS: you can use multiple clients, but you must include at least one client.
# possible values: "brevo", "nodemailer", "telegram"
# example1: brevo
# example2: telegram
# example3:
# let's assume you want to use multiple clients: brevo,nodemailer,telegram
USE_CLIENTS=

# BREVO_API_KEY: your brevo api key
BREVO_API_KEY=

# BREVO_SENDER_EMAIL: the email address of the sender. you first need to add your sender email to brevo.
BREVO_SENDER_EMAIL=

# BREVO_SENDER_NAME: the name of the sender.
BREVO_SENDER_NAME=

# BREVO_TO_EMAIL: the email address of the recipient.
BREVO_TO_EMAIL=

# NODEMAILER_SMTP_HOST: SMTP server host
NODEMAILER_SMTP_HOST=

# NODEMAILER_SMTP_PORT: SMTP server port
NODEMAILER_SMTP_PORT=

# NODEMAILER_SMTP_SECURE: use TLS/SSL (true or false)
NODEMAILER_SMTP_SECURE=

# NODEMAILER_SMTP_AUTH_USER: SMTP auth username
NODEMAILER_SMTP_AUTH_USER=

# NODEMAILER_SMTP_AUTH_PASS: SMTP auth password
NODEMAILER_SMTP_AUTH_PASS=

# NODEMAILER_SENDER_EMAIL: the email address of the sender.
NODEMAILER_SENDER_EMAIL=

# NODEMAILER_TO_EMAIL: the email address of the recipient.
NODEMAILER_TO_EMAIL=

# TELEGRAM_BOT_TOKEN: your telegram bot token from @BotFather
TELEGRAM_BOT_TOKEN=

# TELEGRAM_CHAT_ID: the chat ID to send messages to (can be user ID, group ID, or channel ID)
TELEGRAM_CHAT_ID=

# TELEGRAM_PARSE_MODE: message formatting mode (optional, defaults to HTML)
# possible values: "HTML", "Markdown", "MarkdownV2"
TELEGRAM_PARSE_MODE=

# TELEGRAM_DISABLE_NOTIFICATION: send messages silently (optional, defaults to false)
# possible values: true, false
TELEGRAM_DISABLE_NOTIFICATION=

# TIMEZONE: iana timezone, default value is Europe/Istanbul
TIMEZONE=